@page "/logout"
@using PundoPH.Service
@inject IJSRuntime JSRuntime
@inject NavigationManager NavigationManager
@inject Service Service



@code {
    protected override async Task OnInitializedAsync()
    {
        await JSRuntime.InvokeVoidAsync("sessionStorage.removeItem","currentUser");
        await JSRuntime.InvokeVoidAsync("sessionStorage.removeItem", "Token");
        Service.User.CurrentUser = null;
        Service.Contribution.Contribution = null;
        NavigationManager.NavigateTo("/");
    }
}
